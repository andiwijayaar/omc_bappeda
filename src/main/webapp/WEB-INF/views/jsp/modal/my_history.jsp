<div id="my_history_modal" class="modal fade" data-backdrop="static" data-keyboard="false" tabindex="-1" role="dialog" aria-labelledby="gridSystemModalLabel">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h4 class="modal-title" id="my-history-title">History Saya, Tanggal </h4>
			</div>
			<div class="form-my-history">
				<form>
					<div class="modal-body form-horizontal">
						<div class="form-group">
						    <label for="tanggal" class="col-sm-4 control-label">Tanggal </label>
						    <div class="col-sm-6">
						    	<label for="tanggal" id="tglMyHistory" class="control-label"></label>
							</div>
						</div>
						<div class="form-group">
						    <label for="pkl" class="col-sm-4 control-label">Pukul </label>
						    <div class="col-sm-6">
						    	<label for="tanggal" id="pkl" class="control-label"></label>
							</div>
						</div>
						<div class="form-group">
						    <label for="ip" class="col-sm-4 control-label">Aktivitas </label>
						    <div class="col-sm-6">
						    	<textarea class="form-control" disabled id="activityMyHistory"></textarea>
							</div>
						</div>
						<div class="form-group">
						    <label for="ip" class="col-sm-4 control-label">Alamat IP </label>
						    <div class="col-sm-6">
						    	<label for="tanggal" id="ipMyHistory" class="control-label"></label>
							</div>
						</div>
						<div class="form-group">
						    <label for="ip" class="col-sm-4 control-label">Browser </label>
						    <div class="col-sm-6">
						    	<textarea class="form-control" disabled id="browserMyHistory"></textarea>
							</div>
						</div>
					</div>
					<div class="modal-footer">
						<button type="button" id="closeMyHistoryModal" class="btn btn-default" data-dismiss="modal">Tutup</button>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>